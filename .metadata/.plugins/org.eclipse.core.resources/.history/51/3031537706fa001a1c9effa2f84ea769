import java.time.LocalDate;

public class Teste {

	public static void main(String[] args) {
		Despesa d01 = new Despesa(LocalDate.of(2020,1,1),"Mercado",10.89);
		Despesa d02 = new Despesa(LocalDate.of(2020,2,27),"Luz",50.77);
		System.out.println(d01);
		System.out.println(d02);

	}

}
